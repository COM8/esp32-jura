{
    "cSpell.words": [
        "GPIO",
        "SSID",
        "esp32jura"
    ],
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.errorSquiggles": "EnabledIfIncludesResolve",
    "editor.renderWhitespace": "all",
    "editor.renderIndentGuides": true,
    "C_Cpp.default.compileCommands": "${workspaceFolder}/esp32/build/compile_commands.json",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/build/**": true
    },
    "cmake.configureOnOpen": false,
    "files.exclude": {
        "esp32/build": true
    },
    "clangd.arguments": [
        "--compile-commands-dir",
        "${workspaceFolder}/esp32/build/compile_commands.json",
        "--clang-tidy"
    ],
    "files.associations": {
        "*.pyx": "python",
        "string_view": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "strstream": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "cinttypes": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "codecvt": "cpp",
        "compare": "cpp",
        "complex": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "csignal": "cpp",
